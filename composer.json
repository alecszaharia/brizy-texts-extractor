{
  "name": "bagrinsergiu/brizy-texts-extractor",
  "description": "Extract and replace texts from html documents",
  "type": "library",
  "autoload": {
    "psr-4": {
      "BrizyTextsExtractor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrizyTextsExtractorTests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-dom": "*",
    "alecszaharia/pre-commit-php-cs": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.19",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alecszaharia/pre-commit-php-cs.git"
    }
  ]
}
